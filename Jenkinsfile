pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
                echo 'Pulling code!' 
            }
        }
        stage('Stage 2') {
            steps {
                echo 'Building Code!' 
            }
        }
        stage('Stage 3') {
            steps {
                echo 'Testing Code!' 
            }
        }
        stage('Stage 4') {
            steps {
                echo 'Deploying Code!' 
            }
        }
    }
}
