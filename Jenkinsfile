pipeline {
    agent any 
    stages {
        stage('Pull Code') {
            steps {
                echo 'Pulling code!' 
            }
        }
        stage('Build') {
            steps {
                echo 'Building Code!' 
            }
        }
        stage('Test') {
            steps {
                echo 'Testing Code!' 
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying Code!' 
            }
        }
    }
}
